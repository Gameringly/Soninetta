[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/android/enable_pan_and_scale_gestures=true

[layer_names]

3d_render/layer_1="Default"
3d_render/layer_2="Player"
3d_render/layer_3="Enemy"
3d_physics/layer_1="Default"
3d_physics/layer_2="Player"
3d_physics/layer_3="IgnorePlayer"
3d_physics/layer_4="FlatGround"
3d_physics/layer_5="MovingPlatform"
3d_physics/layer_6="NoWalkGround"
3d_physics/layer_9="TriggersForPlayer"

[physics]

common/physics_ticks_per_second=180
3d/physics_engine="JoltPhysics3D"
common/physics_interpolation=true
jolt_3d/collisions/report_all_kinematic_contacts=true
jolt_3d/solver/contact_speculative_distance=1.0

[rendering]

textures/vram_compression/import_etc2_astc=true
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=5
lights_and_shadows/directional_shadow/16_bits=false
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=5
lightmapping/bake_performance/region_size=1024
anti_aliasing/quality/msaa_3d=3
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_debanding=true
occlusion_culling/use_occlusion_culling=true
renderer/quality/screen_space_aa=0
renderer/quality/msaa_3d=0
