config_version=5

[application]

config/name="Soninetta"
config/version="0.1.1"
run/main_scene="res://Games/Example game/Levels/MainMenu/main_menu.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3", "C#", "Mobile")
boot_splash/image="res://SonicAssets/Images/splashscreen.png"
config/icon="res://SonicAssets/Images/BadnikFrameworkIcon.png"

[debug]

file_logging/enable_file_logging=true
file_logging/log_path="user://logs/badnikframework.log"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=2
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="SonicBadnikFramework"

[editor_plugins]

enabled=PackedStringArray("res://addons/GPUTrail-main/plugin.cfg")

[filesystem]

import/fbx/enabled=false

[importer_defaults]

animation_library={
"animation/trimming": true
}

